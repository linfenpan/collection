{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": []
    },
    "source": {
        "include": ["js", "package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
        "name": "说明文档"
    },
    "opts": {
        "destination": "./docs2/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/tui-jsdoc-template"
    }
}
